  "authors": [
    "Baneina"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/flight-affinity/icon.png",
  "environment": "*",
  "entrypoints": {
